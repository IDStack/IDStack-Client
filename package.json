{
  "name": "IDStack",
  "version": "2.0.0",
  "description": "FYP",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/logo-mini.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"IDStack\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/logo-mini.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/IDStack/IDStack-Client-v2",
  "keywords": [
    "IDStack",
    "FYP"
  ],
  "author": "IDStack",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^8.7.2",
    "protractor": "^5.2.0"
  }
}
